\n        .forEach((key, i) => {\r\n        currentState = states[key];\r\n        result.set(key, iterate(Object.assign({}, currentState, { key })));\r\n        if (currentState && currentState.states && Object.keys(currentState.states).length) {\r\n            iterateState(currentState.states, iterate, depth);\r\n        }\r\n    });\r\n    return result;\r\n}\r\nexport function toArray(value) {\r\n    if (Array.isArray(value)) {\r\n        return value;\r\n    }\r\n    if (value === undefined) {\r\n        return [];\r\n    }\r\n    return [value];\r\n}\r\nexport function ObjToArray(value) {\r\n    if (!value) {\r\n        return [];\r\n    }\r\n    let arr = [];\r\n    arr = Object.keys(value).map(v => {\r\n        const obj = {};\r\n        obj[v] = value[v];\r\n        return obj;\r\n    });\r\n    return arr;\r\n}\r\n//# sourceMappingURL=utils.js.map","dtsmap":{"name":"/Users/luxin/Develop/toy/windmill-fsm/src/utils.d.ts.map","writeByteOrderMark":false,"text":"{\"version\":3,\"file\":\"utils.d.ts\",\"sourceRoot\":\"\",\"sources\":[\"utils.ts\"],\"names\":[],\"mappings\":\"AAAA,wBAAgB,YAAY,CACxB,MAAM,EAAE,GAAG,EACX,OAAO,EAAE,CAAC,WAAW,EAAE,GAAG,KAAK,IAAI,EACnC,KAAK,EAAE,MAAM,iBAsChB;AAED,wBAAgB,OAAO,CAAC,CAAC,EAAE,KAAK,EAAE,CAAC,EAAE,GAAG,SAAS,GAAG,CAAC,GAAG,CAAC,EAAE,CAQ1D;AAED,wBAAgB,UAAU,CAAC,KAAK,EAAE,MAAM,GAAG,SAAS,GAAG,MAAM,EAAE,CAY9D\"}"},"dts":{"name":"/Users/luxin/Develop/toy/windmill-fsm/src/utils.d.ts","writeByteOrderMark":false,"text":"export declare function iterateState(states: any, iterate: (stateConfig: any) => void, depth: number): Map<any, any>;\r\nexport declare function toArray<T>(value: T[] | undefined | T): T[];\r\nexport declare function ObjToArray(value: object | undefined): object[];\r\n//# sourceMappingURL=utils.d.ts.map"}}
