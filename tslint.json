{
    "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
    "linterOptions": {
      "exclude": [
        "config/**/*.js",
        "node_modules/**/*.ts",
        "coverage/lcov-report/*.js"
      ]
    },
    "rules": {
      "no-console": {
        "severity": "warning"
      },
      "no-unused-expression": false,
      "no-bitwise": false,
      "max-classes-per-file": false
    }
  }
  